<html>
    <head> 
        <title><%= name %></title>
		<link rel="stylesheet" href="/style/w3.css" type="text/css" />
        <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=true&key=AIzaSyAig4gbOhqVl-HXa-4v0DlQrfMXZn5YslA"></script>
        <script type="text/javascript">
            var loadMap = function() 
            {
				var myLatLng = {lat: <%=lat%>, lng: <%=lon%>};
                var map = new google.maps.Map(document.getElementById("map"),{
				      zoom: <%=zoom%>,
				      center: myLatLng
				}); 
				var map = new google.maps.Marker({
					position: myLatLng,
					map: map,
				});
            };
            window.onload= loadMap;
        </script>
    </head>
    <body>
		<div class="w3-container w3-teal">
			<h1><%= name %></h1>
		</div>
		<div id="map" style="width:100%;height:420px;"></div>
    </body>
</html>
